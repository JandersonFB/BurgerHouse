<div class="container">
    <div class="card">
        <div class="card-body">
            <h2>Vizualizar Mensagens</h2>
            <hr>
        </div>
        <div class="card-body">
            <div class="row col-12">
                {{#each msg}}
                <div class="form-group col-12">
                    <h5><strong>Stage: </strong>{{dataValues.stage}}</h5>
                    <h5><strong>Mensagem: </strong>{{dataValues.message}}</h5>
                </div>
                <hr>
                {{/each}}
            </div>
        </div>
    </div>
</div>